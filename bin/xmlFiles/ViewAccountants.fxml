<AnchorPane prefHeight="525.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="applicationControllers.ViewAccountants_Controller">
   <children>
      <ScrollPane prefHeight="500.0" prefWidth="500.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="500.0">
               <children>
                  <ListView fx:id="accountantListName" layoutY="26.0" prefHeight="475.0" prefWidth="160.0" />
                  <ListView fx:id="accountantListEmail" layoutX="160.0" layoutY="26.0" prefHeight="475.0" prefWidth="170.0" />
                  <ListView fx:id="accountantListContact" layoutX="330.0" layoutY="26.0" prefHeight="475.0" prefWidth="160.0" />
                  <Label alignment="CENTER" layoutY="-3.0" prefHeight="31.0" prefWidth="160.0" text="Name">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" layoutX="330.0" layoutY="-1.0" prefHeight="31.0" prefWidth="160.0" text="Contact">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" layoutX="165.0" layoutY="-3.0" prefHeight="31.0" prefWidth="160.0" text="Email">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <Button fx:id="viewAcctButton" layoutX="109.0" layoutY="504.0" mnemonicParsing="false" onAction="#ViewAccount" prefHeight="31.0" prefWidth="310.0" text="View Accountant">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="backToAdd" layoutY="504.0" mnemonicParsing="false" onAction="#ViewAccount" prefHeight="31.0" prefWidth="110.0" text="Add Accountant" />
      <Button fx:id="backToAdminSectionFromView" layoutX="417.0" layoutY="504.0" mnemonicParsing="false" onAction="#ViewAccount" prefHeight="31.0" prefWidth="85.0" text="Back" />
   </children>
</AnchorPane>
